<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="menu_shop_aboutus" model="website.menu">
            <field name="name">About Us</field>
            <field name="url">/aboutus</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence">55</field>
        </record>


        <template id="about-us" name="About us">
            <t t-name="website.aboutus">
                <t t-call="website.layout">
                    <div id="wrap" class="oe_structure oe_empty">
                        <section class="pt16 pb16">
                            <div class="container">
                                <div class="row s_nb_column_fixed">
                                    <div class="col-lg-12 s_title about_box pt120">
                                        <h1 class="text-center pt64 about_head">Team</h1>
                                        <div class="s_btn text-right pt48 pb16" data-name="Button">
                                            <a href="#" class="btn btn_aboutus o_default_snippet_text">DO YOU WANNA BE A
                                                PART
                                            </a>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </section>
                        <section class="pt16 pb16">
                            <div class="container">
                                <div class="row s_nb_column_fixed">
                                    <div class="col-lg-12 s_title about_box pt120">
                                        <h1 class="text-center pt64 about_head">Projects</h1>
                                        <div class="s_btn text-right pt48 pb16" data-name="Button">
                                            <a href="/plant-a-tree" class="btn btn_aboutus o_default_snippet_text">PLANT
                                                A TREE
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </t>
            </t>

        </template>
    </data>
</odoo>